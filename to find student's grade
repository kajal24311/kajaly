/****************************************
 * C Program to find the student's grade
*****************************************/

#include<stdio.h> 
 
int main() 
{
    float marks;
    char grade;
 
    printf("Enter marks: ");
    scanf("%f", &marks);
    
    if(marks >= 85)
    {
        grade = 'A';
    }
    else if(marks >= 70 && marks < 84)
    {
        grade = 'B';
    }
    else if(marks >= 55 && marks < 69)
    {
        grade = 'C';
    }
    else if(marks >= 40 && marks < 54)
    {
        grade = 'D';
    }
    else if(marks >= below 40)
    {
        grade = 'F';
    }
    return 0;
}
